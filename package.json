{
  "name": "jedi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch & npm run scss & npm run serve",
    "scss": "node-sass -w ./docs/scss -o ./docs/css",
    "unit": "jest --config=./tests/unit/jest.config.js",
    "e2e": "codeceptjs run-workers 10 -c tests/e2e/codecept.conf.js",
    "e2e:b3": "THEME='bootstrap3' npm run e2e",
    "e2e:b4": "THEME='bootstrap4' npm run e2e",
    "e2e:b5": "THEME='bootstrap5' npm run e2e",
    "e2e:*": "npm run e2e && npm run e2e:b3 && npm run e2e:b4 && npm run e2e:b5",
    "e2e:grep": "SHOW=true THEME='bootstrap5' codeceptjs run -c tests/e2e/codecept.conf.js --steps --debug --grep '@editor-disable'",
    "serve": "reload --port 8282 --browser --watch-dir ./docs --dir ./docs",
    "watch": "webpack --config config/webpack.config.js --watch",
    "webpack": "webpack",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "jsdoc": "jsdoc -r -X ./src > ./docs/jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/germanbisurgi/jedi.git"
  },
  "keywords": [],
  "author": "German Bisurgi <gebisurgi@protonmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/germanbisurgi/jedi/issues"
  },
  "homepage": "https://github.com/germanbisurgi/jedi#readme",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.9.2",
    "babel-loader": "^8.0.6",
    "bootstrap": "^5.3.0",
    "codeceptjs": "^3.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.5",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^29.4.1",
    "jsdoc": "^4.0.2",
    "less": "^4.1.2",
    "less-watch-compiler": "^1.16.3",
    "node-sass": "^9.0.0",
    "puppeteer": "^13.5.2",
    "reload": "^3.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "xmlhttprequest": "^1.8.0"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}
